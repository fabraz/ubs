
<h1>Unidades Básicas de Saúde</h1>

<%= form_tag basic_units_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search" %>
  </p>
<% end %>
<%= notice %>
<table>
  <thead>
    <th>Name</th>
    <th colspan="3"></th>
  </thead>
  <tbody>
    <% @basic_unit.each do |basic_unit| %>
      <tr>
        <td><%= basic_unit.estab_name %></td>
        <td><%= link_to 'Perfil', basic_unit_path(basic_unit) %></td>
      </tr>
    <%end%>
  </tbody>
</table>



